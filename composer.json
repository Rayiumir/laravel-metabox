{
    "name": "rayiumir/laravel-metabox",
    "description": "Easy creation of MetaBox for Laravel",
    "keywords": [
        "laravel-metabox",
        "laravel metabox",
        "easy metabox",
        "laravel",
        "laravel metabox package"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Raymond Baghumian",
            "email": "rayiumir@gmail.com",
            "homepage": "https://github.com/Rayiumir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "^1.4.0|^2.0.0|^3.1.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rayiumir\\LaravelMetabox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rayiumir\\LaravelMetabox\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rayiumir\\LaravelMetabox\\ServiceProvider\\MetaboxServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html ./coverage"
    }
}
